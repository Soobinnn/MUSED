<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<constant name="struts.multipart.maxSize" value="104857600" />
	<package name="talent" extends="struts-default">    <!-- 소진 -->
		<!-- 재능 목록 -->
		<action name="talentList" class="talent.ListAction">
			<result>/talent/ListForm.jsp</result>
		</action>
		
		<!-- 재능 등록 폼 -->
		<action name="talentWriteForm" class="talent.WriteAction" method="form">
			<result>/talent/WriteForm.jsp</result>
		</action>
		<!-- 재능 등록 처리 -->
		<action name="talentWrite" class="talent.WriteAction">
			<result>/talent/WriteSuccessForm.jsp</result>
		</action>
		<!-- 
		<action name="productDetail" class="product.DetailAction" method="form">
			<result>/product/DetailForm.jsp</result>
		</action>
			 -->
	</package>

</struts>